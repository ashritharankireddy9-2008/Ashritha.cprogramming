//program to swap two numbers
#include<stdio.h>
main()
{
	int a,b;
	printf("enter the number (a and b)");
	scanf("%d %d",&a,&b);
	printf("before swap a=%d,b=%d\n",a,b);
	a=a+b;//a now stores the sum of oruginal a and b
	b=a-b;//b now stores the oruginal value of a (sum-original b)
	a=a-b;//a now stores the oruginal value of b (sum-original a)
	printf("after swapping a=%d,b=%d\n",a,b);
}
