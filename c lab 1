//program to take a character,a string and a sentence as input in c
#include<stdio.h>
main()
{
	char ch;
	char s[100];
	char sen[100];
	printf("enter character");
	scanf("%c",&ch);
	scanf("f\n");
	printf("enter string");2
	scanf("%s",s);
	printf("enter sentence");
	scanf("%[^\n]%*c",&sen);
	printf("sen");
}
