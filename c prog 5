//sum and difference of two numbers ,by considering two integer and two floating point number */
#include<stdio.h>
main()
{
	int p,q ;
	float a,b ;
	printf("enter two integer");
	scanf("%d%d",&p,&q) ;
	printf("enter two floating point numbers");
	scanf("%f%f,&a,&b");
	printf("the sum of 2 integer is %d\n",p+q);
    printf("the difference 2 integers is %d\n",p-q);
	printf("the sum 2 floats is %f\n",a+b);
	printf("the difference of 2 floats is%f\n",a-b);	
}.
