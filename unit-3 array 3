//prograam to find total marks and percentage
#include<stdio.h>
main()
{
	int i, marks[6],sum=0;float per;
	for(i=0;i<6;i++)
	{
		printf("enter sub %d marks",i+1);
		scanf("%d",&marks [i]);
	}
	for(i=0;i<6;i++)
	{
		sum=sum+marks[i];
	}
	per=(sum/600.0)*100;
	printf("the total marks : %f",sum);
	printf("the percentage is %f",per);
}
